{
  "name": "Wintermute",
  "colors": {
    "editor.background": "#13242e",
    "editor.foreground": "#bed8e2",
    "activityBar.background": "#163c52",
    "activityBar.inactiveForeground": "#9bd3e9",
    "activityBarBadge.background": "#992376",
    "sideBar.background": "#102c3b",
    "sideBarTitle.foreground": "#9bd3e9",
    "titleBar.activeBackground": "#102c3b",
    "tab.inactiveBackground": "#021927",
    "activityBarTop.background": "#ff0000",
    "statusBarItem.errorBackground": "#a53509",
    "statusBar.background": "#163c52",
    "editorGroupHeader.tabsBackground": "#13242e",
    "menu.background": "#102c3b",
    "editorError.foreground": "#00000000",
    "editorError.background": "#c7594a50",
    "errorLens.errorBackground": "#c7594a50",
    "editorWarning.foreground": "#00000000",
    "editorWarning.background": "#df943850",
    "errorLens.warningBackground": "#df943850",
    "editorInfo.foreground": "#00000000",
    "editorInfo.background": "#60928c50",
    "errorLens.infoBackground": "#60928c50",
    "terminal.ansiBrightGreen": "#94fda2",
    "terminal.ansiBrightBlue": "#75ffff",
    "editorBracketHighlight.foreground1": "#1c92b6",
    "editorBracketHighlight.foreground2": "#bd69c9",
    "editorBracketHighlight.foreground3": "#da8d4f",
    "editorBracketHighlight.foreground4": "#4fa092",
    "editorBracketHighlight.foreground5": "#c7594a",
    "editorBracketHighlight.foreground6": "#cac988"
  },
  "tokenColors": [
    {
      "name": "Strings",
      "scope": ["string.quoted"],
      "settings": {
        "foreground": "#da8d4f"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type.class.python",
        "meta.function-call.generic.python"
      ],
      "settings": {
        "foreground": "#4fa092"
      }
    },
    {
      "name": "Functions, Methods",
      "scope": [
        "entity.name.function.python",
        "meta.function.python",
        "meta.function-call",
        "entity.name.function.decorator",
      ],
      "settings": {
        "foreground": "#cac988"
      }
    },
    {
      "name": "Keywords",
      "scope": ["storage.type.function.python", "storage.type.class.python"],
      "settings": {
        "foreground": "#1c92b6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": ["meta.function.parameters", "meta.attribute"],
      "settings": {
        "foreground": "#bed8e2"
      }
    },
    {
      "name": "Self",
      "scope": [
        "variable.parameter.function.language.special.self.python",
        "variable.language.special.self.python"
      ],
      "settings": {
        "foreground": "#1c92b6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Classes",
      "scope": [],
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#dae7e7"
      }
    },
    {
      "name": "Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#4fa092",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Control",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#bd69c9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment"],
      "settings": {
        "foreground": "#2b5d7a",
        "fontStyle": "italic"
      }
    }
  ]
}
